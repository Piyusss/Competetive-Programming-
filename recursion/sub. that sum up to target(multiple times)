void findCombination(ll ind,ll target,vll &arr,vector<vll> &ans,vll &ds){
	if(ind==arr.size()){
		if(target==0){
			ans.pb(ds);
		}
		return;
	}
	
	//pick-up the element
	if(arr[ind]<=target){
		ds.pb(arr[ind]);
		findCombination(ind,target-arr[ind],arr,ans,ds);
		ds.pop_back();
	}
	findCombination(ind+1,target,arr,ans,ds);
}
