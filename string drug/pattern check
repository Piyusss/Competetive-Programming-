#include <iostream>
#include <string>
using namespace std;

int main() {
    string str, pattern;
    cin >> str >> pattern;
    size_t found = str.find(pattern); // Find pattern in the string
    if (found != string::npos)
        cout << "Pattern found at index: " << found << endl;
    else
        cout << "Pattern not found" << endl;
    return 0;
}
