void allprimes(int n){  //O(nloglog(n)) n is included
    vector<bool>v(n,1);
    v[0]=v[1]=false;
    for(int i=2;i*i<=n;i++){
        if(v[i]){
            for(int j=i*i;j<=n;j+=i){
                v[j]=false;
            }
        }
    }
    for (int i = 2; i <= n; i++)
        if (v[i])
            cout << i << " ";
}
